{
   "name": "migrations",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^3.10.1",
      "@typescript-eslint/parser": "^3.10.1",
      "eslint": "^7.7.0",
      "eslint-config-prettier": "^6.11.0",
      "eslint-plugin-prettier": "^3.1.4",
      "hasura-cli": "^1.3.1",
      "prettier": "^2.1.1"
   },
   "dependencies": {
      "changed-git-files": "0.0.1",
      "pg": "^8.3.3",
      "reflect-metadata": "^0.1.10",
      "ts-node": "^9.0.0",
      "tsconfig-paths": "^3.9.0",
      "typeorm": "0.2.25",
      "typescript": "^4.0.2"
   },
   "scripts": {
      "services:db": "docker-compose -f docker-compose.yaml up -d test-db",
      "services:hasura": "docker-compose -f docker-compose.yaml up -d test-hasura",
      "hasura:init": "hasura init hasura --endpoint",
      "hasura:migrate": "cd hasura && hasura migrate create",
      "hasura:migrations:status": "cd hasura && hasura migrate status",
      "hasura:console": "cd hasura && hasura console",
      "hasura:apply": " cd hasura && hasura migrate apply"
   }
}
